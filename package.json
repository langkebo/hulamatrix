{
  "$schema": "https://json.schemastore.org/jsconfig",
  "name": "hula",
  "type": "module",
  "version": "3.0.5",
  "license": "Apache-2.0",
  "engines": {
    "node": "^20.19.0 || >=22.12.0",
    "pnpm": ">=10.x"
  },
  "author": {
    "name": "HuLaSpark团队",
    "email": "2439646234@qq.com"
  },
  "scripts": {
    "========= 启动vue(tauri项目会连带执行不需要单独执行) =========": "",
    "dev": "vite",
    "========= 打包vue(tauri项目会连带执行不需要单独执行) =========": "",
    "build": "vite build",
    "typecheck": "vue-tsc -p tsconfig.json --noEmit",
    "rust:check": "cargo check --manifest-path src-tauri/Cargo.toml",
    "rust:clippy": "cargo clippy --manifest-path src-tauri/Cargo.toml -- -D warnings",
    "rust:test": "cargo test --manifest-path src-tauri/Cargo.toml",
    "typecheck:tests": "tsc -p tsconfig.tests.json --noEmit",
    "typecheck:module": "vue-tsc --noEmit",
    "typecheck:strict": "vue-tsc -p tsconfig.strict.json --noEmit",
    "typecheck:deep": "node scripts/deep-type-check.cjs",
    "typecheck:fix": "node scripts/fix-common-type-issues.cjs",
    "========= 启动HuLa桌面应用程序 =========": "",
    "tauri:dev": "tauri dev",
    "========= 启动HuLa桌面应用程序(简化命令) =========": "",
    "td": "tauri dev",
    "========= windows中启动安卓纯前端(简化命令) =========": "",
    "adev:win": "set TAURI_ENV_PLATFORM=android&& vite",
    "========= mac中启动ios纯前端(简化命令) =========": "",
    "idev:mac": "TAURI_ENV_PLATFORM=ios vite",
    "========= 启动安卓程序(简化命令) =========": "",
    "adev": "tauri android dev",
    "========= 启动ios程序(简化命令) =========": "",
    "idev": "tauri ios dev",
    "========= 打包桌面应用程序(简化命令) =========": "",
    "tb": "node scripts/interactive-build-inquirer.js",
    "========= 打包桌面应用程序 =========": "",
    "tauri:build": "node scripts/interactive-build-inquirer.js",
    "========= 启动HuLa ios 程序 =========": "",
    "tauri:ios:dev": "tauri ios dev",
    "========= 启动HuLa android 程序 =========": "",
    "tauri:android:dev": "tauri android dev",
    "========= 生成icon =========": "",
    "tauri:icon": "tauri icon public/light.png",
    "========= 安装依赖前执行 =========": "",
    "preinstall": "npx only-allow pnpm && node scripts/check-all.js",
    "========= 使用commit来进行代码提交 =========": "",
    "commit": "git add . && git-cz",
    "========= 只检查代码问题和格式(不修复) =========": "",
    "check": "biome check",
    "========= 修复代码问题和格式并且检查 =========": "",
    "check:write": "biome check --write --unsafe",
    "========= 格式化Vue文件模板 =========": "",
    "format:vue": "npx prettier --write \"src/**/*.vue\"",
    "========= 完整格式化(JS+Vue) =========": "",
    "format:all": "pnpm check:write && pnpm format:vue",
    "========= Git提交前的代码检查(简化命令) =========": "",
    "lint:s": "lint-staged && vue-tsc --noEmit",
    "========= Git提交前的代码检查 =========": "",
    "lint:staged": "lint-staged && vue-tsc --noEmit",
    "lint:msg": "node scripts/check-msg.cjs",
    "========= 安装husky =========": "",
    "prepare": "husky",
    "========= 发版 =========": "",
    "release": "release-it",
    "========= commit后再次添加修改到上一次的commit =========": "",
    "addition-commit": "git add . && git commit --amend --no-edit",
    "========= 单元测试 =========": "",
    "test:run": "vitest run --maxConcurrency=1 --isolate",
    "========= Phase 4: WebSocket 迁移测试 =========": "",
    "test:websocket-disabled": "node scripts/validate-websocket-disabled.mjs",
    "========= 使用 vitest UI =========": "",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "========= 测试覆盖率 =========": "",
    "coverage": "vitest run --coverage",
    "========= 单独提交 =========": "",
    "gitcz": "git-cz",
    "lint:full": "biome check",
    "lint:eslint": "eslint . --ext .ts,.tsx,.vue",
    "quality:gate": "node scripts/quality/run-quality-gates.cjs",
    "analyze:type": "node scripts/quality/scan-typecheck.cjs",
    "========= UI/UX 检查和修复 =========": "",
    "uiux:check": "node scripts/ui-ux-check.cjs",
    "uiux:fix": "node scripts/ui-ux-fix.cjs",
    "uiux:fix:dry": "node scripts/ui-ux-fix.cjs --dry-run",
    "uiux:tokens": "node scripts/check-design-tokens.cjs",
    "uiux:images": "node scripts/optimize-images.cjs",
    "uiux:images:dry": "node scripts/optimize-images.cjs --dry-run",
    "========= UI/UX 质量检查全流程 =========": "",
    "uiux:audit": "pnpm uiux:check && pnpm uiux:tokens && pnpm uiux:images:dry"
  },
  "dependencies": {
    "@tauri-apps/api": "2.9.0",
    "@tauri-apps/plugin-autostart": "2.5.1",
    "@tauri-apps/plugin-barcode-scanner": "2.4.2",
    "@tauri-apps/plugin-clipboard-manager": "2.3.2",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-global-shortcut": "^2.3.1",
    "@tauri-apps/plugin-http": "2.5.4",
    "@tauri-apps/plugin-log": "^2.7.1",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-opener": "^2.5.2",
    "@tauri-apps/plugin-os": "2.3.2",
    "@tauri-apps/plugin-process": "2.3.1",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "@tauri-apps/plugin-sql": "^2.3.1",
    "@tauri-apps/plugin-updater": "2.9.0",
    "@types/dompurify": "^3.2.0",
    "@vicons/tabler": "^0.13.0",
    "@vueuse/components": "^14.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "digest-wasm": "^0.1.4",
    "dompurify": "^3.3.0",
    "es-toolkit": "^1.41.0",
    "file-type": "^21.0.0",
    "hula-emojis": "^1.2.31",
    "matrix-js-sdk": "file:../matrix-js-sdk-39.1.3",
    "mitt": "^3.0.1",
    "naive-ui": "^2.43.2",
    "p-limit": "^7.2.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "pinia-shared-state": "^1.0.1",
    "seemly": "^0.3.10",
    "tauri-plugin-mic-recorder-api": "^2.0.0",
    "vant": "^4.9.22",
    "vue": "^3.5.25",
    "vue-cropper": "^0.6.5",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vue-virtual-scroller": "^1.1.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@iconify/vue": "^5.0.0",
    "@inquirer/prompts": "^7.8.6",
    "@playwright/test": "^1.57.0",
    "@tauri-apps/cli": "2.9.4",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@unocss/preset-wind3": "^66.5.4",
    "@unocss/reset": "^66.5.4",
    "@unocss/transformer-directives": "^66.5.4",
    "@unocss/transformer-variant-group": "^66.5.4",
    "@unocss/vite": "^66.5.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^14.0.0",
    "chalk": "^5.6.2",
    "commitlint": "20.3.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^10.6.2",
    "fast-check": "^4.4.0",
    "glob": "^13.0.0",
    "happy-dom": "^20.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.6.2",
    "release-it": "^17.11.0",
    "sass": "1.94.2",
    "sharp": "^0.34.5",
    "typescript": "5.9.0-beta",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "7.2.4",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^4.0.8",
    "vue-tsc": "^3.1.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "tlbs-map-vue",
      "vue-demi"
